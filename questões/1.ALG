Algoritmo "semnome"

Var
n,n2: inteiro


Inicio
escreval("digite um numero")
leia(n)
se (n < 0) entao
   n2 <- n*3

   escreval("seu numero é ",n2)
senao
     se (n > 0) entao
        n2 <- n*2

        escreval("seu numero é ",n2)
     senao
         escreval("seu numero esta invalido")
     fimse
fimse






Fimalgoritmo